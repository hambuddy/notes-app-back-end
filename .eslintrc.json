{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "@typescript-eslint/no-unused-vars": "off",
        "@typescript-eslint/no-explicit-any": "off",
        "indent": "off",
        "quotes": "off",
        "linebreak-style": "off",
        "lines-between-class-members": "off",
        "no-multiple-empty-lines": "off",
        "comma-dangle": "off",
        "no-underscore-dangle": "off",
        "no-unused-vars": "off",
        "eol-last": "off",
        "typescript-eslint/no-unused-vars": "off",
        "semi": "off"
    }
}
